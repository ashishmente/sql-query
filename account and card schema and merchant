  CREATE TABLE "FCMCORE"."FCM_MERCHANT" 
   (	"MERCHANT_NUMBER" VARCHAR2(20 BYTE), 
	"COMMERCIAL_NAME" VARCHAR2(100 BYTE), 
	"TYPE_OF_BUSINESS" VARCHAR2(50 BYTE), 
	"NATURE_OF_BUSINESS" VARCHAR2(100 BYTE), 
	"HIGH_RISK_FLAG" VARCHAR2(3 BYTE), 
	"KEY_ACCOUNT_FLAG_VIP" VARCHAR2(3 BYTE), 
