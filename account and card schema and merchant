	"TELEPHONE_NUMBER" VARCHAR2(20 BYTE), 
	"CITY_NAME" VARCHAR2(50 BYTE), 
	"ACCOUNT_NUMBER" VARCHAR2(20 BYTE), 
	"ACCOUNT_NAME" VARCHAR2(100 BYTE), 
	"MERCHANT_ACCOUNT_CURRENCY" VARCHAR2(10 BYTE), 
	"BANK_SWIFT_CODE" VARCHAR2(20 BYTE), 
	"MERCHANT_ACCOUNT_PROFILE" VARCHAR2(20 BYTE), 
	"MCC" VARCHAR2(20 BYTE), 
	 PRIMARY KEY ("MERCHANT_NUMBER")
