
  CREATE TABLE "FCMCORE"."FCM_ACCOUNT" 
   (	"CUSTOMER_ID" VARCHAR2(20 BYTE), 
	"ACCOUNT_TYPE" VARCHAR2(20 BYTE), 
	"ACCOUNT_IBAN_NUM" VARCHAR2(25 BYTE), 
	"ACCOUNT_HOLDER_NAME" VARCHAR2(100 BYTE), 
	"ACCOUNT_STATUS" VARCHAR2(20 BYTE), 
	"ACCOUNT_CURRENCY" VARCHAR2(10 BYTE), 
	"MATURITY_DATE" DATE, 
	"ACCOUNT_MANAGER" VARCHAR2(100 BYTE), 
	"ACCOUNT_AVAILABLE_BALANCE" NUMBER, 
	"ACCOUNT_CURRENT_BALANCE" NUMBER, 
	"LAST_DEPOSIT_DATE" DATE, 
	"ACCOUNT_LAST_DEPOSIT" NUMBER, 
	"LAST_WITHDRAWL_DATE" DATE, 
	"ACCOUNT_LAST_WITHDRAWL" NUMBER, 
	"LINKED_DEBIT_CARD_1" VARCHAR2(20 BYTE), 
	"PRIMARY_CONTACT_NUM" VARCHAR2(20 BYTE), 
	"PRIMARY_EMAIL" VARCHAR2(100 BYTE), 
	"EMAIL_SEND" VARCHAR2(20 BYTE) DEFAULT 'NO', 
	 PRIMARY KEY ("ACCOUNT_IBAN_NUM")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 163840 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SOR_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 163840 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SOR_DATA" ;


  CREATE TABLE "FCMCORE"."FCM_CARD" 
   (	"CUSTOMER_ID" VARCHAR2(20 BYTE), 
	"CARD_NUM" VARCHAR2(23 BYTE), 
	"CARDHOLDER_NAME" VARCHAR2(100 BYTE), 
	"CARD_CREDIT_LIMIT" NUMBER(*,0), 
	"TOTAL_AUTHORIZATION_LIMIT" NUMBER(*,0), 
	"CASH_AUTHORIZATION_LIMIT" NUMBER(*,0), 
	"CARD_BONUS_TOTAL" NUMBER(*,0), 
	"BONUS_START_DATE" DATE, 
	"BONUS_END_DATE" DATE, 
	"CARD_OPENING_DATE" DATE, 
	"ONLINE_AUTHORIZATION_CARD_STATUS" VARCHAR2(20 BYTE), 
	"CUSTOMER_INCOME" NUMBER(*,0), 
	"CUSTOMER_TYPE" VARCHAR2(20 BYTE), 
	"NATIONALITY" VARCHAR2(50 BYTE), 
	"CUSTOMER_MOBILE_NUM_DEFAULT_ADDRESS" VARCHAR2(50 BYTE), 
	"EMAIL_ADDRESS_DEFAULT" VARCHAR2(100 BYTE), 
	"DATE_OF_BIRTH" DATE, 
	"GENDER" CHAR(1 BYTE), 
	"EMAIL_SEND" VARCHAR2(20 BYTE) DEFAULT 'NO', 
	 PRIMARY KEY ("CARD_NUM")
  USING INDEX PCTFREE 10 INITRANS 2 MAXTRANS 255 COMPUTE STATISTICS 
  STORAGE(INITIAL 163840 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
  BUFFER_POOL DEFAULT FLASH_CACHE DEFAULT CELL_FLASH_CACHE DEFAULT)
  TABLESPACE "SOR_DATA"  ENABLE
   ) SEGMENT CREATION IMMEDIATE 
  PCTFREE 10 PCTUSED 40 INITRANS 1 MAXTRANS 255 
 NOCOMPRESS LOGGING
  STORAGE(INITIAL 163840 NEXT 1048576 MINEXTENTS 1 MAXEXTENTS 2147483645
  PCTINCREASE 0 FREELISTS 1 FREELIST GROUPS 1
